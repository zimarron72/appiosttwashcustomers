        }, {
          key: "_validateHints",
          value: function _validateHints() {
            var _this10 = this;

            if (this._hintChildren && (typeof ngDevMode === 'undefined' || ngDevMode)) {
              var startHint;
              var endHint;

              this._hintChildren.forEach(function (hint) {
