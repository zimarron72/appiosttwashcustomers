} from \"@angular/material/snack-bar\";\nimport { Router } from '@angular/router';\nimport {ServicioAutenticacion} from '../shared/servicio.autenticacion'\n\n@Component({\n  selector: 'app-resetpassword',\n  templateUrl: './resetpassword.page.html',\n  styleUrls: ['./resetpassword.page.scss'],\n})\nexport class ResetpasswordPage implements OnInit {\n  form_reset : FormGroup\n  constructor(\n\n    private snackBar: MatSnackBar,\n    private formBuilder: FormBuilder,\n    private servicioauth : ServicioAutenticacion,\n    private router: Router,\n\n  ) { }\n\n  ngOnInit() {\n    this.reactiveForm()\n  }\n\n  reactiveForm() {\n  \n    this.form_reset = this.formBuilder.group({\n      email: [, { validators: [Validators.required]}],    \n      \n    });\n  }\n\n\n  submit() {\n \n    if (this.form_reset.valid) {\n  \n    \n    var email = this.form_reset.get(\"email\").value;\n     \n      return this.servicioauth.resetpassword(email)\n  \n    }\n  \n    else {\n      this.snackBar.open(\"Please, this value is required\", \"Close\",\n      {       \n        horizontalPosition: \"start\",\n        verticalPosition: \"top\",\n      })\n    }\n  \n    }\n\n    cancel() {\n\n      this.router.navigate(['/login']);\n    \n    }\n\n}\n","export default \".etiqueta {\\n  color: #fff;\\n}\\n\\nion-content {\\n  --ion-background-color:#cb033c;\\n}\\n\\n.app-logo {\\n  background-image: url(https://washtt.com/app_logo_inicio.png);\\n  height: 150px;\\n  width: 150px;\\n  background-repeat: no-repeat;\\n  background-position: center;\\n  background-size: contain;\\n  margin: 20px auto;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \"<ion-header>\\n  <ion-toolbar color=\\\"primary\\\">\\n    <img src=\\\"./assets/imgs/logo_invert.jpg\\\">\\n    <ion-title>Password recovery</ion-title>\\n  </ion-toolbar>\\n  <ion-toolbar>\\n    <ion-icon name=\\\"information-circle\\\" slot='start' color=\\\"primary\\\"></ion-icon>\\n    <ion-title size=\\\"small\\\